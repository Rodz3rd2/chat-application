<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>{{ config('app.name') }}</title>
</head>
<body>

    <script type="text/javascript">
        var conn = new WebSocket("ws://localhost:8888");
        conn.onopen = function (e)
        {
            console.log("Connection established!");
        };

        conn.onmessage = function (e)
        {
            console.log(e.data);
        };
    </script>
</body>
</html>